{
	"name": "practice-hub-backend",
	"version": "1.0.0",
	"description": "考研刷题小程序后端服务",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"dev": "nest start",
		"start": "node dist/main.js",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"create-admin": "ts-node -r tsconfig-paths/register scripts/create-admin.ts",
		"export:csv": "ts-node -r tsconfig-paths/register scripts/export-to-csv.ts",
		"export:sql": "ts-node -r tsconfig-paths/register scripts/export-to-sql.ts",
		"import:csv": "ts-node -r tsconfig-paths/register scripts/import-from-csv.ts",
		"import:remote": "ts-node -r tsconfig-paths/register scripts/import-to-remote.ts",
		"migrate": "node scripts/smart-migration.js",
		"fix:home-recommend": "node scripts/fix-home-recommend-item.js",
		"check:remote": "node scripts/check-remote-db.js",
		"migrate:remote": "node scripts/migrate-remote-db.js",
		"check:wechat": "node scripts/check-wechat-config.js",
		"test:wechat": "node scripts/test-wechat-login.js",
		"fix:answer-log": "node scripts/add-user-answer-log-fields.js",
		"fix:answer-log:remote": "node scripts/add-user-answer-log-fields.js --remote",
		"fix:is-correct": "node scripts/fix-is-correct-nullable.js",
		"fix:is-correct:remote": "node scripts/fix-is-correct-nullable.js --remote",
		"fix:chapter-id": "node scripts/add-chapter-id-to-user-answer-log.js",
		"fix:chapter-id:remote": "node scripts/add-chapter-id-to-user-answer-log.js --remote",
		"migrate:distribution": "node scripts/create-distribution-tables.js",
		"migrate:distribution:remote": "node scripts/create-distribution-tables.js --remote",
		"migrate:exam": "node scripts/create-exam-tables.js",
		"migrate:exam:remote": "node scripts/create-exam-tables.js --remote",
		"test-data": "node scripts/insert-test-data.js",
		"test-data:remote": "node scripts/insert-test-data.js --remote",
		"verify-data": "node scripts/verify-test-data.js",
		"verify-data:remote": "node scripts/verify-test-data.js --remote",
		"delete-test-data": "node scripts/delete-test-data.js",
		"delete-test-data:remote": "node scripts/delete-test-data.js --remote",
		"delete-questions": "node scripts/delete-test-questions.js",
		"delete-questions:remote": "node scripts/delete-test-questions.js --remote",
		"insert-english": "node scripts/insert-english-questions.js",
		"insert-english:remote": "node scripts/insert-english-questions.js --remote",
		"fix:foreign-key": "node scripts/fix-chapter-foreign-key.js",
		"fix:foreign-key:remote": "node scripts/fix-chapter-foreign-key.js --remote"
	},
	"dependencies": {
		"@nestjs/common": "^10.3.0",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^10.3.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.3.0",
		"@nestjs/schedule": "^4.0.0",
		"@nestjs/swagger": "^7.1.17",
		"@nestjs/typeorm": "^10.0.1",
		"axios": "^1.6.2",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cos-nodejs-sdk-v5": "^2.15.4",
		"exceljs": "^4.4.0",
		"mammoth": "^1.6.0",
		"mysql2": "^3.6.5",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.17"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.2.1",
		"@nestjs/schematics": "^10.0.3",
		"@nestjs/testing": "^10.3.0",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.11",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.10.6",
		"@types/passport-jwt": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"jest": "^29.7.0",
		"prettier": "^3.1.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
